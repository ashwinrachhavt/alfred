APP_ENV=dev
SECRET_KEY=dev
REDIS_URL=redis://localhost:6379/0

# Qdrant (cloud) optional
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION=alfred_docs

# OpenAI (required for embeddings/LLM)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Notion (optional)
NOTION_TOKEN=
NOTION_PARENT_PAGE_ID=
NOTION_CLIENTS_DB_ID=
NOTION_NOTES_DB_ID=

# Gmail integration (optional)
ENABLE_GMAIL=false
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=
GOOGLE_PROJECT_ID=
GOOGLE_SCOPES=["https://www.googleapis.com/auth/gmail.readonly","https://www.googleapis.com/auth/gmail.metadata"]
GCP_PUBSUB_TOPIC=
TOKEN_STORE_DIR=.alfred_data/tokens

# CORS
CORS_ALLOW_ORIGINS=["*"]

# Web search providers (optional)
BRAVE_SEARCH_API_KEY=
EXA_API_KEY=
TAVILY_API_KEY=
YDC_API_KEY=

# Supabase 
SUPABASE_URL=https://<your-project>.supabase.co
SUPABASE_ANON_KEY=<anon-public-key>          # for user-scoped calls (RLS enforced)
SUPABASE_SERVICE_ROLE_KEY=<service-role-key> # for admin tasks (bypass RLS)
